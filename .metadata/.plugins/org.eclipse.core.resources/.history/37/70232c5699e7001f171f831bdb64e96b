function goto(url) {
top.location = url;
}

$(function () {

	
	loadGraph();

});

function loadGraph() {
	
	jQuery.getJSON( self.local, displayGraph);
}


function displayGraph(data, status) {
	var points = [];
		
	alert("geht auch hier durch");	
	for(i in data) {
		
		points.push([data[i].time, data[i].value]);
	}

    $.plot($("#graphcanvas"), [
        {
            data: points,
            lines: { show: true, steps: true },
            points: { show: false }
        }
    ], {
     xaxis: {
    		 mode: "time"
     }
    } );
 }



